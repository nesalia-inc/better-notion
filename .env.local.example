# Environment variables for integration tests
# Copy this file to .env.local and fill in your values

# Notion API token (required)
# Create at https://notion.so/my-integrations
NOTION_KEY=secret_your_token_here

# (Optional) Existing database ID for testing
# If set, tests will use this database instead of creating a new one
# NOTION_TEST_DATABASE_ID=your_database_id_here

# (Optional) Existing page ID for testing
# NOTION_TEST_PAGE_ID=your_page_id_here
